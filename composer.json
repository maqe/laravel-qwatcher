{
    "name": "maqe/laravel-qwatcher",
    "description": "A full-lifecycle queue jobs watcher for Laravel 5.1+",
    "keywords": ["queue", "job", "laravel", "watcher"],
    "license": "proprietary",
    "authors": [
        {
            "name": "MAQE team",
            "email": "hello@maqe.com",
            "homepage": "https://www.maqe.com/"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/bus": "~5.1",
        "illuminate/database": "~5.1",
        "illuminate/queue": "~5.1",
        "illuminate/support": "~5.1"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0",
        "mockery/mockery": "~0.9"
    },
    "autoload": {
        "classmap": [
            "src/",
            "database/",
            "tests/TestCase.php"
        ],
        "psr-4": {
            "Maqe\\Qwatcher\\": "src/",
            "Maqe\\Qwatcher\\Tests\\": "tests/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    }
}
